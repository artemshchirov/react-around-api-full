{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": 0,
    "no-undef": "warn",
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "import/no-extraneous-dependencies": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": [
      "warn",
      {
        "allow": ["_id"]
      }
    ]
  }
}
